g(response1.data);\r\n      this.articles = response1.data;\r\n      const response2 = await NewsService.getNewsType(this.category, 2);\r\n      console.log(\"in categories: service called\");\r\n      console.log(response2.data);\r\n      this.articles = this.articles.concat(response2.data); // Append data from page 2 to existing articles\r\n      // .then((response) => {\r\n      //   console.log(\"in categories: service called\");\r\n      //   console.log(response.data);\r\n      //   this.articles = response.data;\r\n\r\n      // })\r\n      // .catch((error) => {\r\n      //   console.error(\"Error fetching data:\", error);\r\n      //   console.log(\"in categories: error\");\r\n      // });\r\n      await NewsService.getNewsType(this.category, 2)\r\n        .then((response) => {\r\n          console.log(\"in categories: service called\");\r\n          console.log(response.data);\r\n          this.articles.concat(response.data);\r\n          // this.articles = response.data;\r\n\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Error fetching data:\", error);\r\n          console.log(\"in categories: error\");\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n    \r\n<style>\r\n.navbar {\r\n  margin-top: -1.5cm;\r\n  background-color: black;\r\n\r\n}\r\n\r\n#item {\r\n  color: white;\r\n  font-size: 21px;\r\n  font-family: Major Mono Display;\r\n  padding-left: 1cm;\r\n\r\n}\r\n</style>"],"mappings":"AAeA,OAAOA,WAAU,MAAO,yBAAwB;AAChD,OAAOC,cAAa,MAAO,sBAAsB,EAAE;AACnD,OAAOC,WAAU,MAAO,mBAAmB;AAC3C,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE,CAAC;MACfC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDL,UAAU,EAAE;IACVF;EACF,CAAC;EACDQ,OAAOA,CAAA,EAAG;IACRC,OAAO,CAACC,GAAG,CAAC,uBAAuB,CAAC;IACpC,IAAI,CAACC,SAAS,CAAC,IAAI,CAACJ,QAAQ,CAAC;EAC/B,CAAC;EACDK,OAAOA,CAAA,EAAG;IACR,IAAI,CAACL,QAAO,GAAI,IAAI,CAACM,MAAM,CAACC,KAAK,CAACP,QAAQ;IAC1C;IACAE,OAAO,CAACC,GAAG,CAAE,sBAA