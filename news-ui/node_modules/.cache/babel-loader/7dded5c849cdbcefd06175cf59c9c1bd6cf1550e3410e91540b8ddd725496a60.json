{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport NewsCard from './NYNewsCard.vue';\nimport NewsService from '../services/NewsService.js';\nimport NYNavFooter from './NYNavFooter.vue';\nexport default {\n  name: 'NewsList',\n  components: {\n    NewsCard,\n    NYNavFooter\n  },\n  data() {\n    return {\n      totalResults: 0,\n      articles: [] // Initialize with an empty array to store news data\n    };\n  },\n\n  mounted() {\n    this.fetchNewsData();\n  },\n  methods: {\n    getNews(category) {\n      console.log(\"passing category to NY Categories page: \", category);\n      this.$router.push({\n        name: 'NYCategories',\n        query: {\n          category: category\n        }\n      });\n    },\n    fetchNewsData() {\n      NewsService.getNews().then(response => {\n        if (response && response.data) {\n          // Check if response and response.data are defined\n          this.totalResults = response.data.totalResults;\n          this.articles = response.data;\n        } else {\n          console.error('Invalid API response:', response);\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["NewsCard","NewsService","NYNavFooter","name","components","data","totalResults","articles","mounted","fetchNewsData","methods","getNews","category","console","log","$router","push","query","then","response","error","catch"],"sources":["C:\\Users\\fatima.noor\\Desktop\\NewsApi-git\\news-ui\\src\\components\\NYNewsList.vue"],"sourcesContent":["<!-- NewsList.vue -->\r\n<template>\r\n   <div>\r\n    <NYNavFooter />\r\n  </div>\r\n  <div>\r\n    <div class=\"news-cards\">\r\n      <news-card v-for=\"(article, index) in articles\" :key=\"index\" :article=\"article\"></news-card>\r\n    </div>\r\n  </div>\r\n</template>\r\n  \r\n<script>\r\nimport NewsCard from './NYNewsCard.vue';\r\nimport NewsService from '../services/NewsService.js';\r\nimport NYNavFooter from './NYNavFooter.vue';\r\nexport default {\r\n\r\n  name: 'NewsList',\r\n  components: {\r\n    NewsCard,\r\n    NYNavFooter,\r\n  },\r\n  data() {\r\n    return {\r\n      totalResults: 0,\r\n      articles: [] // Initialize with an empty array to store news data\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchNewsData();\r\n  },\r\n  methods: {\r\n    getNews(category) {\r\n      console.log(\"passing category to NY Categories page: \", category);\r\n      this.$router.push({name:'NYCategories', query: { category: category }});\r\n    },\r\n    fetchNewsData() {\r\n      NewsService.getNews()\r\n        .then((response) => {\r\n          if (response && response.data) {\r\n            // Check if response and response.data are defined\r\n            this.totalResults = response.data.totalResults;\r\n            this.articles = response.data;\r\n          } else {\r\n            console.error('Invalid API response:', response);\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        })\r\n    },\r\n  }\r\n}\r\n</script>\r\n  \r\n<style scoped>\r\n/* Add styles for category selector and news cards container here */\r\n.category-selector {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.category-selector button {\r\n  background-color: #007BFF;\r\n  color: #fff;\r\n  border: none;\r\n  padding: 10px 20px;\r\n  margin: 0 10px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.category-selector button:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.news-cards {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n}\r\n</style>\r\n  "],"mappings":";AAaA,OAAOA,QAAO,MAAO,kBAAkB;AACvC,OAAOC,WAAU,MAAO,4BAA4B;AACpD,OAAOC,WAAU,MAAO,mBAAmB;AAC3C,eAAe;EAEbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVJ,QAAQ;IACRE;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,CAAC;MACfC,QAAQ,EAAE,EAAC,CAAE;IACf,CAAC;EACH,CAAC;;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,