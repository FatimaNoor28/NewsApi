{"ast":null,"code":"import NewsService from \"../services/NewsService\";\nimport NYCategoryCard from \"./NYCategoryCard.vue\"; // Import the NewsCard component\nimport NYNavFooter from \"./NYNavFooter.vue\";\nexport default {\n  name: 'NYCategories',\n  data() {\n    return {\n      articles: [],\n      totalResults: 0,\n      displayInCol: 0,\n      category: ''\n    };\n  },\n  components: {\n    NYCategoryCard,\n    NYNavFooter\n  },\n  mounted() {\n    console.log(\"in categories mounted\");\n    this.fetchNews(this.category);\n  },\n  created() {\n    this.category = this.$route.query.category;\n    // Use the 'category' variable in your component logic\n    console.log(`Category received: ${this.category}`);\n  },\n  watch: {\n    $route(to, from) {\n      if (to.query.category !== from.query.category) {\n        this.category = to.query.category;\n        this.fetchNews(this.category);\n      }\n    }\n  },\n  methods: {\n    async fetchNews(category) {\n      this.category = category;\n      await NewsService.getNewsType(this.category).then(response => {\n        console.log(\"in categories: service called\");\n        console.log(response.data);\n        this.articles = response.data;\n        this.firstHalfItems();\n        // this.totalResults = response.data.length;\n        // this.articles = response.data;\n      }).catch(error => {\n        console.error(\"Error fetching data:\", error);\n        console.log(\"in categories: error\");\n      });\n    },\n    firstHalfItems() {\n      this.displayInCol = this.articles.length / 3;\n      this.totalResults = Math.floor((this.articles.length - this.displayInCol) / 3);\n      if ((this.articles.length - this.displayInColumn) % 3 != 0) {\n        this.displayInCol += (this.articles.length - this.displayInCol) % 3;\n      }\n    }\n  }\n};","map":{"version":3,"names":["NewsService","NYCategoryCard","NYNavFooter","name","data","articles","totalResults","displayInCol","category","components","mounted","console","log","fetchNews","created","$route","query","watch","to","from","methods","getNewsType","then","response","firstHalfItems","catch","error","length","Math","floor","displayInColumn"],"sources":["C:\\Users\\fatima.noor\\Desktop\\NewsApi-git\\news-ui\\src\\components\\NYCategories.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <NYNavFooter />\r\n  </div>\r\n  <div class=\"news-cards\">\r\n    <h1>News Section</h1>\r\n    <div class=\"item\" v-if=\"articles.length > 0\">\r\n      <NYCategoryCard v-for=\"(article, index) in articles\" :key=\"index\" :totalResults=\"totalResults\" :displayInCol=\"displayInCol\" :article=\"article\"\r\n        :category=\"category\">\r\n      </NYCategoryCard>\r\n    </div>\r\n  </div>\r\n</template>\r\n    \r\n<script>\r\nimport NewsService from \"../services/NewsService\"\r\nimport NYCategoryCard from \"./NYCategoryCard.vue\"; // Import the NewsCard component\r\nimport NYNavFooter from \"./NYNavFooter.vue\";\r\nexport default {\r\n  name: 'NYCategories',\r\n  data() {\r\n    return {\r\n      articles: [],\r\n      totalResults: 0,\r\n      displayInCol:0,\r\n      category: '',\r\n    };\r\n  },\r\n  components: {\r\n    NYCategoryCard,\r\n    NYNavFooter,\r\n  },\r\n  mounted() {\r\n    console.log(\"in categories mounted\");\r\n    this.fetchNews(this.category);\r\n  },\r\n  created() {\r\n    this.category = this.$route.query.category;\r\n    // Use the 'category' variable in your component logic\r\n    console.log(`Category received: ${this.category}`);\r\n  },\r\n  watch: {\r\n    $route(to, from) {\r\n      if (to.query.category !== from.query.category) {\r\n        this.category = to.query.category;\r\n        this.fetchNews(this.category);\r\n      }\r\n    },\r\n  },\r\n  methods: {\r\n\r\n    async fetchNews(category) {\r\n      this.category = category;\r\n     \r\n      await NewsService.getNewsType(this.category)\r\n        .then((response) => {\r\n          console.log(\"in categories: service called\");\r\n          console.log(response.data);\r\n          this.articles = response.data;\r\n          this.firstHalfItems();\r\n          // this.totalResults = response.data.length;\r\n          // this.articles = response.data;\r\n\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Error fetching data:\", error);\r\n          console.log(\"in categories: error\");\r\n        });\r\n    },\r\n    firstHalfItems() {\r\n      this.displayInCol = this.articles.length / 3;\r\n      this.totalResults = Math.floor((this.articles.length - this.displayInCol) / 3);\r\n\r\n      if ((this.articles.length - this.displayInColumn) % 3 != 0) {\r\n        this.displayInCol += (this.articles.length - this.displayInCol) % 3;\r\n\r\n      }\r\n\r\n    },\r\n  },\r\n};\r\n</script>\r\n    \r\n<style>\r\n.navbar {\r\n  margin-top: -1.5cm;\r\n  background-color: black;\r\n\r\n}\r\n\r\n#item {\r\n  color: white;\r\n  font-size: 21px;\r\n  font-family: Major Mono Display;\r\n  padding-left: 1cm;\r\n\r\n}\r\n</style>\r\n\r\n"],"mappings":"AAeA,OAAOA,WAAU,MAAO,yBAAwB;AAChD,OAAOC,cAAa,MAAO,sBAAsB,EAAE;AACnD,OAAOC,WAAU,MAAO,mBAAmB;AAC3C,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE,CAAC;MACfC,YAAY,EAAC,CAAC;MACdC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IACVR,cAAc;IACdC;EACF,CAAC;EACDQ,OAAOA,CAAA,EAAG;IACRC,OAAO,CAACC,GAAG,CAAC,uBAAuB,CAAC;IACpC,IAAI,CAACC,SAAS,CAAC,IAAI,CAACL,QAAQ,CAAC;EAC/B,CAAC;EACDM,OAAOA,CAAA,EAAG;IACR,IAAI,CAACN,QAAO,GAAI,IAAI,CAACO,MAAM,CAACC,KAAK,CAACR,QAAQ;IAC1C;IACAG,OAAO,CAACC,GAAG,CAAE,sBAAqB,IAAI,CAACJ,QAAS,EAAC,CAAC;EACpD,CAAC;EACDS,KAAK,EAAE;IACLF,MAAMA,CAACG,EAAE,EAAEC,IAAI,EAAE;MACf,IAAID,EAAE,CAACF,KAAK,CAACR,QAAO,KAAMW,IAAI,CAACH,KAAK,CAACR,QAAQ,EAAE;QAC7C,IAAI,CAACA,QAAO,GAAIU,EAAE,CAACF,KAAK,CAACR,QAAQ;QACjC,IAAI,CAACK,SAAS,CAAC,IAAI,CAACL,QAAQ,CAAC;MAC/B;IACF;EACF,CAAC;EACDY,OAAO,EAAE;IAEP,MAAMP,SAASA,CAACL,QAAQ,EAAE;MACxB,IAAI,CAACA,QAAO,GAAIA,QAAQ;MAExB,MAAMR,WAAW,CAACqB,WAAW,CAAC,IAAI,